{
    "name": "No Thank You",
    "description": "Disable Service Workers, stop video autoplay",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_start",
            "all_frames": true,
            "match_about_blank": true,
            "js": ["contentscript.js"]
        }
    ]
}
